<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/constraintLayoutMonthly"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textViewMonthTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text=""
        app:layout_constraintVertical_bias="0.025"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/rowZero"
        layout="@layout/calendar_monthly_abrv_row"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_width="390dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/textViewMonthTitle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


    <include
        android:id="@+id/rowOne"
        layout="@layout/calendar_monthly_row"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_width="390dp"
        android:layout_height="65dp"
        app:layout_constraintTop_toBottomOf="@id/rowZero"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <include
        android:id="@+id/rowTwo"
        layout="@layout/calendar_monthly_row"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_width="390dp"
        android:layout_height="65dp"
        app:layout_constraintTop_toBottomOf="@id/rowOne"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <include
        android:id="@+id/rowThree"
        layout="@layout/calendar_monthly_row"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_width="390dp"
        android:layout_height="65dp"
        app:layout_constraintTop_toBottomOf="@id/rowTwo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <include
        android:id="@+id/rowFour"
        layout="@layout/calendar_monthly_row"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_width="390dp"
        android:layout_height="65dp"
        app:layout_constraintTop_toBottomOf="@id/rowThree"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <include
        android:id="@+id/rowFive"
        layout="@layout/calendar_monthly_row"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_width="390dp"
        android:layout_height="65dp"
        app:layout_constraintTop_toBottomOf="@id/rowFour"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <include
        android:id="@+id/rowSix"
        layout="@layout/calendar_monthly_row"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_width="390dp"
        android:layout_height="65dp"
        app:layout_constraintTop_toBottomOf="@id/rowFive"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</android.support.constraint.ConstraintLayout>